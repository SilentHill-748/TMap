<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Map="clr-namespace:TMap.MVVM.ViewModel.Map"
                    xmlns:Settings="clr-namespace:TMap.MVVM.ViewModel.Settings"
                    xmlns:SettingViews="clr-namespace:TMap.MVVM.View.Settings"
                    xmlns:Views="clr-namespace:TMap.MVVM.View">
    <DataTemplate DataType="{x:Type Map:MapViewModel}">
        <Views:MapView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type Settings:MapSettingsViewModel}">
        <SettingViews:MapSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type Settings:RoadSettingsViewModel}">
        <SettingViews:RoadSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type Settings:PipelineChannelSettingsViewModel}">
        <SettingViews:PipelineChannelSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type Settings:PipeSettingsViewModel}">
        <SettingViews:PipeSettingsView/>
    </DataTemplate>
</ResourceDictionary>