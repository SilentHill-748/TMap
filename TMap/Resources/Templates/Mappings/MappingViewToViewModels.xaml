<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Map="clr-namespace:TMap.MVVM.ViewModel.Map"
                    xmlns:SettingsMap="clr-namespace:TMap.MVVM.ViewModel.Settings.Map"
                    xmlns:SettingsRoad="clr-namespace:TMap.MVVM.ViewModel.Settings.Road"
                    xmlns:SettingsPipeline="clr-namespace:TMap.MVVM.ViewModel.Settings.Pipeline"
                    xmlns:SettingsPipelineChannel="clr-namespace:TMap.MVVM.ViewModel.Settings.PipelineChannel"
                    xmlns:SettingViews="clr-namespace:TMap.MVVM.View.Settings"
                    xmlns:Views="clr-namespace:TMap.MVVM.View">
    <DataTemplate DataType="{x:Type Map:MapViewModel}">
        <Views:MapView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type SettingsMap:MapSettingsViewModel}">
        <SettingViews:MapSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type SettingsRoad:RoadSettingsViewModel}">
        <SettingViews:RoadSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type SettingsPipelineChannel:PipelineChannelSettingsViewModel}">
        <SettingViews:PipelineChannelSettingsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type SettingsPipeline:PipeSettingsViewModel}">
        <SettingViews:PipeSettingsView/>
    </DataTemplate>
</ResourceDictionary>